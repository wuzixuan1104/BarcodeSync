{
  "name": "web-boilerplate",
  "version": "0.0.1",
  "description": "",
  "engines": {
    "node": ">=8.15.1"
  },
  "license": "MIT",
  "scripts": {
    "extract-intl": "node ./tools/scripts/extract-intl.js",
    "prebuild": "yarn build:clean",
    "build": "cross-env NODE_ENV=production webpack --config tools/webpack/webpack.prod.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "presetup": "yarn add chalk shelljs",
    "setup": "node tools/scripts/setup.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server  --config tools/webpack/webpack.dev.js",
    "start:production": "yarn test && yarn build && yarn start:prod",
    "start:prod": "cross-env NODE_ENV=production webpack-dev-server",
    "clean": "yarn test:clean && yarn build:clean",
    "make": "plop --plopfile tools/generators/index.js",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:css": "stylelint src/**/*.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern tools/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern tools/scripts --fix",
    "lint:js": "yarn lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "yarn test:clean && yarn lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write",
    "release": "semantic-release --no-ci"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.3",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "chalk": "^2.4.2",
    "connected-react-router": "6.8.0",
    "cross-env": "5.2.0",
    "crypto-js": "^4.0.0",
    "fontfaceobserver": "2.1.0",
    "history": "4.9.0",
    "hoist-non-react-statics": "3.3.2",
    "immer": "3.0.0",
    "intl": "1.2.5",
    "invariant": "2.2.4",
    "lodash": "4.17.11",
    "micro-graphql-react": "^0.4.0-rc.2",
    "minimist": "1.2.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-intl": "^4.7.5",
    "react-pouchdb": "^2.1.0",
    "react-redux": "7.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.0.1",
    "redux-saga": "1.0.2",
    "reselect": "4.0.0",
    "sanitize.css": "8.0.0",
    "semantic-release": "^17.0.2",
    "semantic-release-gitmoji": "^1.3.2",
    "shelljs": "^0.8.3",
    "styled-components": "^4.3.2",
    "workbox-sw": "^5.1.3",
    "workbox-webpack-plugin": "^5.1.3",
    "workbox-window": "^5.1.3",
    "pouchdb": "^7.2.2"
  },
  "devDependencies": {
